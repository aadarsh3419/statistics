from scipy.stats import f_oneway

# Sample data: Exam scores for 3 different teaching methods
method_A = [85, 88, 90, 93, 95]
method_B = [78, 82, 84, 80, 79]
method_C = [92, 94, 96, 91, 90]

# Perform ANOVA
f_stat, p_value = f_oneway(method_A, method_B, method_C)

print("F-statistic:", f_stat)
print("p-value:", p_value)

if p_value < 0.05:
    print("Conclusion: At least one teaching method differs significantly.")
else:
    print("Conclusion: No significant difference between methods.")
